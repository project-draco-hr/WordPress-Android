}
catch (SSLHandshakeException e) {
if (!WPUrlUtils.isWordPressCom(xmlrpcUri.toString())) {
throw new WPOrgUtilsException(WPOrgUtilsException.Kind.ERRONEOUS_SSL_CERTIFICATE,0,xmlrpcUri.toString(),null);
