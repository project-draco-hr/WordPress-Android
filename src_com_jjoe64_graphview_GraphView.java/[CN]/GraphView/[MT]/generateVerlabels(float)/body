{
  int numLabels=(int)(graphheight / GraphViewConfig.HORIZONTAL_LABEL_HEIGHT);
  String[] labels=new String[numLabels + 1];
  double min=getMinY();
  double max=getMaxY();
  if (max == min) {
    max=max * 1.05d;
    min=min * 0.95d;
  }
  for (int i=0; i <= numLabels; i++) {
    labels[numLabels - i]=formatLabel(min + ((max - min) * i / numLabels),false);
  }
  return labels;
}
