{
  double smallest;
  if (manualYAxis) {
    smallest=manualMinYValue;
  }
 else {
    smallest=Integer.MAX_VALUE;
    for (int i=0; i < graphSeries.size(); i++) {
      GraphViewData[] values=_values(i);
      for (int ii=0; ii < values.length; ii++)       if (values[ii].valueY < smallest)       smallest=values[ii].valueY;
    }
  }
  return smallest;
}
