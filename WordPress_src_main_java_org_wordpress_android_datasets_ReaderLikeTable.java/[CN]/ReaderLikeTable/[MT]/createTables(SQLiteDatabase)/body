{
  db.execSQL("CREATE TABLE tbl_post_likes (" + " post_id        INTEGER," + " blog_id        INTEGER,"+ " user_id        INTEGER,"+ " PRIMARY KEY (blog_id, post_id, user_id))");
  db.execSQL("CREATE TABLE tbl_comment_likes (" + " comment_id     INTEGER," + " blog_id        INTEGER,"+ " user_id        INTEGER,"+ " PRIMARY KEY (blog_id, comment_id, user_id))");
}
