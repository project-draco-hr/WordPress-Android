{
  if (tagName == null || tagName.length() == 0) {
    return "";
  }
  if (Character.isUpperCase(tagName.charAt(0))) {
    return tagName;
  }
  if (tagName.length() > 1 && Character.isLowerCase(tagName.charAt(0)) && Character.isUpperCase(tagName.charAt(1))) {
    return tagName;
  }
  return StringUtils.capitalize(tagName);
}
