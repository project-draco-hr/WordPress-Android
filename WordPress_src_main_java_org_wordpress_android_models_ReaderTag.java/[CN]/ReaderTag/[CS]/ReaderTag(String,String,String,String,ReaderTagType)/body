{
  if (TextUtils.isEmpty(slug)) {
    if (tagType == ReaderTagType.DEFAULT && !TextUtils.isEmpty(title)) {
      setTagSlug(title);
    }
 else {
      setTagSlug(getTagSlugFromEndpoint(endpoint));
    }
  }
 else {
    setTagSlug(slug);
  }
  setTagDisplayName(displayName);
  setTagTitle(title);
  setEndpoint(endpoint);
  this.tagType=tagType;
}
