{
  if (isDataEmpty()) {
    return false;
  }
  SearchTermsModel searchTerms=(SearchTermsModel)mDatamodels[0];
  if (searchTerms.getSearchTerms() != null && searchTerms.getSearchTerms().size() > 0) {
    return true;
  }
  if (searchTerms.getEncryptedSearchTerms() > 0) {
    return true;
  }
  return false;
}
