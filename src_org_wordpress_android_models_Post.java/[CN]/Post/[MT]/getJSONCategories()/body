{
  JSONArray jArray=null;
  if (categories == null) {
    categories="[]";
  }
  try {
    categories=StringUtils.unescapeHTML(categories);
    if (TextUtils.isEmpty(categories)) {
      jArray=new JSONArray(categories);
    }
 else {
      jArray=new JSONArray();
    }
  }
 catch (  JSONException e) {
    AppLog.e(T.POSTS,e);
  }
  return jArray;
}
