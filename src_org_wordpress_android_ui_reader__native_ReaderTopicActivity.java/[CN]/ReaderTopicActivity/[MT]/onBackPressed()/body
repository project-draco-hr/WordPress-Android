{
  if (mTopicsChanged) {
    Bundle bundle=new Bundle();
    bundle.putBoolean(KEY_TOPICS_CHANGED,true);
    if (mLastAddedTopicName != null && ReaderTopicTable.topicExists(mLastAddedTopicName))     bundle.putString(KEY_LAST_ADDED_TOPIC,mLastAddedTopicName);
    Intent intent=new Intent();
    intent.putExtras(bundle);
    setResult(RESULT_OK,intent);
  }
  super.onBackPressed();
}
