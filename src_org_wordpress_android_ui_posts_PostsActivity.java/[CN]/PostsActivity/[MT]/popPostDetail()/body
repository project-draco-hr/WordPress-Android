{
  if (isFinishing()) {
    return;
  }
  FragmentManager fm=getSupportFragmentManager();
  ViewPostFragment f=(ViewPostFragment)fm.findFragmentById(R.id.postDetail);
  if (f == null) {
    try {
      fm.popBackStack();
    }
 catch (    RuntimeException e) {
      AppLog.e(T.POSTS,e);
    }
  }
}
