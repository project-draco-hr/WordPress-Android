def action_open_notifications(device, serialno):
    print 'open notifications'
    for i in range(5):
        device.press('KEYCODE_DPAD_UP')
    device.press('KEYCODE_DPAD_DOWN')
    device.press('KEYCODE_DPAD_DOWN')
    device.press('KEYCODE_ENTER')
    time.sleep(1)
    device.press('KEYCODE_DPAD_DOWN')
    time.sleep(10)
