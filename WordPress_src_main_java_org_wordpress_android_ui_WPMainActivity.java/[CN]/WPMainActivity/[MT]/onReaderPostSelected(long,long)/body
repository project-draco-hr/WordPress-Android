{
  ReaderPostListFragment listFragment=getReaderListFragment();
  if (listFragment == null) {
    return;
  }
switch (listFragment.getPostListType()) {
case TAG_FOLLOWED:
case TAG_PREVIEW:
    ReaderActivityLauncher.showReaderPostPagerForTag(this,listFragment.getCurrentTag(),listFragment.getPostListType(),blogId,postId);
  break;
case BLOG_PREVIEW:
ReaderActivityLauncher.showReaderPostPagerForBlog(this,blogId,postId);
break;
}
}
