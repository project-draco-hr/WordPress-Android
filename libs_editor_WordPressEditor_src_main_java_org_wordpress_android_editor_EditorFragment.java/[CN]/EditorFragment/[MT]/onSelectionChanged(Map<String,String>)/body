{
  final String id=selectionArgs.get("id");
  mWebView.post(new Runnable(){
    @Override public void run(){
      if (!id.isEmpty()) {
switch (id) {
case "zss_field_title":
          updateToolbarEnabledState(false);
        break;
case "zss_field_content":
      updateToolbarEnabledState(true);
    break;
}
}
}
}
);
}
