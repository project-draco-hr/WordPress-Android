{
  if (jsonObject == null) {
    return;
  }
  new Thread(){
    @Override public void run(){
      ReaderUrlList urls=new ReaderUrlList();
      JSONArray jsonBlogs=jsonObject.optJSONArray("subscriptions");
      if (jsonBlogs != null) {
        for (int i=0; i < jsonBlogs.length(); i++)         urls.add(JSONUtil.getString(jsonBlogs.optJSONObject(i),"URL"));
      }
      ReaderBlogTable.setFollowedBlogUrls(urls);
    }
  }
.start();
}
