{
  if (mDatamodels == null) {
    return false;
  }
  FollowersModel followersModel=(FollowersModel)mDatamodels[mTopPagerSelectedButtonIndex];
  if (followersModel == null || followersModel.getFollowers() == null || followersModel.getFollowers().size() < 10) {
    return false;
  }
  return true;
}
