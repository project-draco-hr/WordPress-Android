{
  try {
    InputStream is=mContext.getAssets().open(filename);
    String data=convertStreamToString(is);
    AppLog.v(T.TESTS,"file read:" + filename);
    return data;
  }
 catch (  IOException e) {
    AppLog.e(T.TESTS,"can't read file: " + filename + " - "+ e.toString());
  }
  return null;
}
