{
  if (isFinishing()) {
    return;
  }
  mIsRequestingMorePosts=false;
  if (hasNewPosts) {
    ReaderBlogIdPostId id=getCurrentBlogIdPostId();
    long blogId=(id != null ? id.getBlogId() : 0);
    long postId=(id != null ? id.getPostId() : 0);
    loadPosts(blogId,postId);
  }
 else {
    mAllPostsLoaded=true;
    checkLastFragment();
  }
}
