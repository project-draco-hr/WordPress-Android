{
  if (!isValidPosition(position)) {
    return null;
  }
  if (position > 0) {
    for (int index=position - 1; index >= 0; index--) {
      if (mIdList.get(index).getBlogId() != blogId) {
        return mIdList.get(index);
      }
    }
  }
  if (position < getCount() - 1) {
    for (int index=position + 1; index < getCount(); index++) {
      if (mIdList.get(index).getBlogId() != blogId) {
        return mIdList.get(index);
      }
    }
  }
  return null;
}
