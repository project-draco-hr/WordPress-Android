{
  if (!isFullScreenSupported() || enableFullScreen == mIsFullScreen) {
    return false;
  }
  if (getSupportActionBar() != null) {
    if (enableFullScreen) {
      getSupportActionBar().hide();
    }
 else {
      getSupportActionBar().show();
    }
  }
  mIsFullScreen=enableFullScreen;
  return true;
}
