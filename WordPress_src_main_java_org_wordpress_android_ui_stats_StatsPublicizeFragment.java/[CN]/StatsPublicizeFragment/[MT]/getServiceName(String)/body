{
  if (service.equals("facebook")) {
    return "Facebook";
  }
  if (service.equals("twitter")) {
    return "Twitter";
  }
  if (service.equals("tumblr")) {
    return "Tumblr";
  }
  if (service.equals("google_plus")) {
    return "Google+";
  }
  if (service.equals("linkedin")) {
    return "LinkedIn";
  }
  if (service.equals("path")) {
    return "Path";
  }
  return StringUtils.capitalize(service);
}
