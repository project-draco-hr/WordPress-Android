{
  int ordinal=getTimeframeOrdinal();
  Uri uri=null;
  if (ordinal == StatsTimeframe.DAYS.ordinal())   uri=StatsContentProvider.STATS_BAR_CHART_DAYS_URI;
 else   if (ordinal == StatsTimeframe.WEEKS.ordinal())   uri=StatsContentProvider.STATS_BAR_CHART_WEEKS_URI;
 else   if (ordinal == StatsTimeframe.MONTHS.ordinal())   uri=StatsContentProvider.STATS_BAR_CHART_MONTHS_URI;
  return uri;
}
