{
  super.onResume();
  if (SimperiumUtils.getNotesBucket() != null) {
    SimperiumUtils.getNotesBucket().addListener(this);
  }
  mTabLayout.checkNoteBadge();
  trackLastVisibleTab(mViewPager.getCurrentItem());
  checkConnection();
  ProfilingUtils.split("WPMainActivity.onResume");
  ProfilingUtils.dump();
  ProfilingUtils.stop();
}
