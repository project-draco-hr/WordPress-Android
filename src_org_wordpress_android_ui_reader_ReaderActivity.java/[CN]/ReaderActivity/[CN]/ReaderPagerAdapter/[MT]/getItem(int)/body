{
  Fragment f=null;
switch (location) {
case 0:
    f=ReaderTopicsSelectorFragment.newInstance();
  topicPage=f;
break;
case 1:
f=ReaderImplFragment.newInstance();
readerPage=f;
break;
case 2:
f=ReaderDetailPageFragment.newInstance();
detailPage=f;
break;
case 3:
f=ReaderWebPageFragment.newInstance();
webPage=f;
break;
}
return f;
}
