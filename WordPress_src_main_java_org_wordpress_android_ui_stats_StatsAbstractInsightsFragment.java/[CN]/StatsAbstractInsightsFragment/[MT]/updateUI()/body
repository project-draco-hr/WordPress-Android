{
  if (!isAdded()) {
    return;
  }
  if (mDatamodels == null) {
    showPlaceholderUI();
    return;
  }
  if (isDataEmpty(0)) {
    showErrorUI(0);
    return;
  }
  mErrorLabel.setVisibility(View.GONE);
  mResultContainer.setVisibility(View.VISIBLE);
  mEmptyModulePlaceholder.setVisibility(View.GONE);
  customizeUIWithResults();
}
