{
  String endpoint=ReaderTagTable.getEndpointForTag(tagName);
  if (TextUtils.isEmpty(endpoint)) {
    if (ReaderTag.isDefaultTagName(tagName)) {
      return null;
    }
 else {
      return String.format("/read/tags/%s/posts",ReaderTagActions.sanitizeTitle(tagName));
    }
  }
 else {
    return endpoint;
  }
}
