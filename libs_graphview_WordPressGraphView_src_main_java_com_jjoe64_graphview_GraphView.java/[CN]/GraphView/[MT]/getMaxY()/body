{
  double largest;
  if (manualYAxis) {
    largest=manualMaxYValue;
  }
 else {
    largest=Integer.MIN_VALUE;
    for (int i=0; i < graphSeries.size(); i++) {
      GraphViewDataInterface[] values=_values(i);
      for (int ii=0; ii < values.length; ii++)       if (values[ii].getY() > largest)       largest=values[ii].getY();
    }
  }
  return largest;
}
