{
  if (!isAdded()) {
    return false;
  }
  if (ContextCompat.checkSelfPermission(getActivity(),permission.ACCESS_FINE_LOCATION) != PackageManager.PERMISSION_GRANTED) {
    ActivityCompat.requestPermissions(getActivity(),new String[]{permission.ACCESS_FINE_LOCATION,permission.ACCESS_COARSE_LOCATION},PERMISSION_REQUEST_LOCATION);
    return false;
  }
  return true;
}
