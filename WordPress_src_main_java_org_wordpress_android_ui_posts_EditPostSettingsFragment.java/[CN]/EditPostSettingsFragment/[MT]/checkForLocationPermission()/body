{
  if (!isAdded()) {
    return false;
  }
  if (ContextCompat.checkSelfPermission(getActivity(),permission_group.LOCATION) != PackageManager.PERMISSION_GRANTED) {
    ActivityCompat.requestPermissions(getActivity(),new String[]{permission_group.LOCATION},PERMISSION_REQUEST_LOCATION);
    return false;
  }
  return true;
}
