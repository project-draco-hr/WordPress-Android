{
  if (this.getChildCount() > column) {
    for (int i=0; i < this.mColCount; i++) {
      final View child=getChildAt(i);
      if (child != null) {
        final int left=child.getLeft();
        int col=0;
        while (left > col * (this.mColWidth + mItemMargin * 2) + getPaddingLeft()) {
          col++;
        }
        if (col == column) {
          return child;
        }
      }
    }
  }
  return null;
}
