{
  String result="LayoutRecord{c=" + column + ", id="+ id+ " h="+ height+ " s="+ span;
  if (mMargins != null) {
    result+=" margins[above, below](";
    for (int i=0; i < mMargins.length; i+=2) {
      result+="[" + mMargins[i] + ", "+ mMargins[i + 1]+ "]";
    }
    result+=")";
  }
  return result + "}";
}
