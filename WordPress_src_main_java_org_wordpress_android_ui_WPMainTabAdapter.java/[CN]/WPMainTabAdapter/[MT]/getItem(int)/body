{
switch (position) {
case TAB_SITES:
    return new DummyFragment();
case TAB_READER:
  return ReaderPostListFragment.newInstance(ReaderTag.getDefaultTag(),ReaderTypes.ReaderPostListType.TAG_FOLLOWED);
case TAB_ME:
return new DummyFragment();
case TAB_NOTIFS:
return new DummyFragment();
default :
return null;
}
}
