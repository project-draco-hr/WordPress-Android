{
  db=ctx.openOrCreateDatabase(DATABASE_NAME,0,null);
  Cursor c=db.query(EULA_TABLE,new String[]{"read"},"id=0",null,null,null,null);
  int numRows=c.getCount();
  c.moveToFirst();
  boolean returnValue=false;
  if (numRows == 1) {
    returnValue=(c.getInt(0) != 0);
  }
  c.close();
  db.close();
  return returnValue;
}
