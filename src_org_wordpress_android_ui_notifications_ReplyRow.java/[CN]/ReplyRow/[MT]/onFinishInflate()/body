{
  mButton=(ImageView)findViewById(R.id.button);
  mAvatar=(ImageView)findViewById(R.id.avatar);
  mTextField=(TextView)findViewById(R.id.text);
  mProgress=(ProgressBar)findViewById(R.id.progress);
