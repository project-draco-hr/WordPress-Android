{
  if (blogInfo == null) {
    return R.drawable.gravatar_placeholder;
  }
 else   if (blogInfo.isJetpack) {
    return R.drawable.blavatar_placeholder_org;
  }
 else   if (blogInfo.isExternal()) {
    return R.drawable.gravatar_placeholder;
  }
 else {
    return R.drawable.blavatar_placeholder_com;
  }
}
