{
switch (item.getItemId()) {
case android.R.id.home:
    if (isLargeOrXLarge()) {
      return super.onOptionsItemSelected(item);
    }
 else {
      FragmentManager fm=getSupportFragmentManager();
      if (fm.getBackStackEntryCount() > 0) {
        fm.popBackStack();
        return true;
      }
    }
  break;
}
return super.onOptionsItemSelected(item);
}
