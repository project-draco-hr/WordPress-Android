{
  super.onStart();
  Preference togglePref=mSettingsFragment.findPreference(getString(org.wordpress.passcodelock.R.string.pref_key_passcode_toggle));
  Preference changePref=mSettingsFragment.findPreference(getString(org.wordpress.passcodelock.R.string.pref_key_change_passcode));
  if (togglePref != null && changePref != null) {
    mPasscodePreferenceFragment.setPreferences(togglePref,changePref);
  }
}
