{
  if (mStatsActivityWeakRef.get() == null || mStatsActivityWeakRef.get().isFinishing() || !mStatsActivityWeakRef.get().mIsInFront) {
    return;
  }
  if (StatsUtils.getBlogId(mLocalBlogID) == null) {
    stopStatsService();
    mPullToRefreshHelper.setRefreshing(false);
    showJetpackMissingAlert(this.mStatsActivityWeakRef.get());
  }
}
