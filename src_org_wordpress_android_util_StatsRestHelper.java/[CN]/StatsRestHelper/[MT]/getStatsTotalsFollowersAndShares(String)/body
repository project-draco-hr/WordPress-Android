{
  getStatsSummary(blogId,new StatsSummaryInterface(){
    @Override public void onSuccess(){
      updateRefreshMap(StatsViewType.TOTALS_FOLLOWERS_AND_SHARES,-1);
    }
    @Override public void onFailure(    VolleyError error){
      updateRefreshMap(StatsViewType.TOTALS_FOLLOWERS_AND_SHARES,-1);
    }
  }
);
}
