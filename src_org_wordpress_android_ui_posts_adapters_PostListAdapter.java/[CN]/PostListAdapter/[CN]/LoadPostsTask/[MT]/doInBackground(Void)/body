{
  if (WordPress.getCurrentBlog() != null) {
    List<PostsListPost> postsList=WordPress.wpDB.getPostsListPosts(WordPress.getCurrentBlog().getLocalTableBlogId(),mIsPage);
    if (postsList.size() == 0) {
      mOnLoadMoreListener.onLoadMore(false);
    }
 else {
      setPosts(postsList);
    }
  }
  return null;
}
