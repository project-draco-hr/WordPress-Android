{
  ImageSize size=getImageSizeFromAttachments(imageUrl);
  if (size != null) {
    return size;
  }
 else   if (imageUrl.contains("?")) {
    return getImageSizeFromQueryParams(imageUrl);
  }
 else   if (imageTag.contains("width=")) {
    return getImageSizeFromAttributes(imageTag);
  }
 else {
    return null;
  }
}
