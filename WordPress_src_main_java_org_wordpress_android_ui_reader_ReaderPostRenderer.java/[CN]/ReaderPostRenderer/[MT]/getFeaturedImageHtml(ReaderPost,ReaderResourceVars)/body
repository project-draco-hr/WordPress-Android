{
  if (resourceVars != null) {
    int width=resourceVars.fullSizeImageWidth;
    int height=resourceVars.featuredImageHeight;
    String imageUrl=ReaderUtils.getResizedImageUrl(post.getFeaturedImage(),width,height,post.isPrivate);
    return String.format("<img class='size-full featured-image' src='%s' width='%d' height='%d' />",imageUrl,width,height);
  }
 else {
    return String.format("<img class='size-full' src='%s' />",post.getFeaturedImage());
  }
}
