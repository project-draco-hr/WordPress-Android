{
  String imageUrl;
  if (resourceVars != null) {
    imageUrl=ReaderUtils.getResizedImageUrl(post.getFeaturedImage(),resourceVars.fullSizeImageWidth,resourceVars.featuredImageHeight,post.isPrivate);
  }
 else {
    imageUrl=post.getFeaturedImage();
  }
  return String.format("<p><img class='size-full' src='%s' /></p>",imageUrl);
}
