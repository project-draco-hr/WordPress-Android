{
  if (post == null) {
    return "";
  }
 else {
    String content=post.getText().replace("src=\"//","src=\"http://");
    if (shouldAddFeaturedImage(post)) {
      AppLog.w(AppLog.T.READER,"reader renderer > added featured image");
      content=getFeaturedImageHtml(post,resourceVars) + content;
    }
    return content;
  }
}
