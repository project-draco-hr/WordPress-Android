{
  super.onActivityResult(requestCode,resultCode,data);
  if (data != null) {
switch (requestCode) {
case 0:
      WordPressDB settingsDB=new WordPressDB(this);
    accounts=settingsDB.getAccounts(this);
  if (accounts.size() == 0) {
    finish();
  }
 else {
    displayAccounts();
  }
break;
}
}
 else {
displayAccounts();
}
}
