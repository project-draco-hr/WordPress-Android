{
  ReaderBlogList blogs=new ReaderBlogList();
  if (json == null) {
    return blogs;
  }
  JSONArray jsonBlogs=json.optJSONArray("subscriptions");
  if (jsonBlogs != null) {
    for (int i=0; i < jsonBlogs.length(); i++)     blogs.add(ReaderBlog.fromJson(jsonBlogs.optJSONObject(i)));
  }
  return blogs;
}
