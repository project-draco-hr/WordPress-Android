{
  Vector<HashMap<String,Object>> accounts=WordPress.wpDB.getAccounts();
  int lastBlogId=WordPress.wpDB.getLastBlogId();
  if (lastBlogId != -1) {
    for (    HashMap<String,Object> account : accounts) {
      int id=Integer.valueOf(account.get("id").toString());
      if (id == lastBlogId) {
        setCurrentBlog(id);
      }
    }
  }
  return currentBlog;
}
