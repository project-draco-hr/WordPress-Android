{
  Vector<HashMap<String,Object>> accounts=WordPress.wpDB.getAccounts(context);
  for (  HashMap<String,Object> account : accounts) {
    int accountId=(Integer)account.get("id");
    if (accountId == id) {
      try {
        return new Blog(id,context);
      }
 catch (      Exception e) {
        e.printStackTrace();
      }
    }
  }
  return null;
}
