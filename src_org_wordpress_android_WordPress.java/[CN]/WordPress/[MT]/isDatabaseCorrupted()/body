{
  try {
    List<Map<String,Object>> accounts=wpDB.getAllAccounts();
    for (    Map<String,Object> account : accounts) {
      if (account == null || account.get("blogName") == null || account.get("url") == null) {
        return true;
      }
    }
  }
 catch (  RuntimeException re) {
    return true;
  }
  return false;
}
