{
  if (mConfig == null) {
    mConfig=new Properties();
    InputStream stream=getResources().openRawResource(R.raw.config);
    try {
      mConfig.load(stream);
    }
 catch (    java.io.IOException e) {
      mConfig=null;
      Log.e(TAG,"Could not load config",e);
    }
  }
  return mConfig;
}
