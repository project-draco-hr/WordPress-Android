{
  if (currentBlog == null) {
    setCurrentBlogToLastActive();
    List<Map<String,Object>> accounts=WordPress.wpDB.getAccounts();
    if (currentBlog == null && accounts.size() > 0) {
      int id=Integer.valueOf(accounts.get(0).get("id").toString());
      setCurrentBlog(id);
      wpDB.updateLastBlogId(id);
    }
  }
  return currentBlog;
}
