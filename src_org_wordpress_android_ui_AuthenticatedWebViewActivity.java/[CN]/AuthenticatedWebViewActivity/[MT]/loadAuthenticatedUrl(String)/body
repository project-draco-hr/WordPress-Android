{
  try {
    String postData=String.format("log=%s&pwd=%s&redirect_to=%s",URLEncoder.encode(mBlog.getUsername(),"UTF-8"),URLEncoder.encode(mBlog.getPassword(),"UTF-8"),URLEncoder.encode(url,"UTF-8"));
    mWebView.postUrl(getLoginUrl(),postData.getBytes());
  }
 catch (  UnsupportedEncodingException e) {
    AppLog.e(T.UTILS,e);
  }
}
