{
  Cursor c=db.query(SETTINGS_TABLE,new String[]{"id"},"api_blogid=" + dotComBlogId,null,null,null,null);
  int id=-1;
  int numRows=c.getCount();
  c.moveToFirst();
  if (numRows > 0) {
    id=c.getInt(0);
  }
  c.close();
  try {
    return new Blog(id);
  }
 catch (  Exception e) {
    return null;
  }
}
