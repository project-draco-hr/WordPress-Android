{
  String mediaIdsStr="(";
  for (  String mediaId : mediaIds) {
    mediaIdsStr+="'" + mediaId + "',";
  }
  mediaIdsStr=mediaIdsStr.subSequence(0,mediaIdsStr.length() - 1) + ")";
  return db.rawQuery("SELECT id as _id, * FROM " + MEDIA_TABLE + " WHERE blogId=? AND mediaId IN "+ mediaIdsStr,new String[]{blogId});
}
