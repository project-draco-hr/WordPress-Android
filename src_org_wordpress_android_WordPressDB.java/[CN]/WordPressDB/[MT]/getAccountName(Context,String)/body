{
  db=ctx.openOrCreateDatabase(DATABASE_NAME,0,null);
  String accountName="";
  Cursor c=db.query(SETTINGS_TABLE,new String[]{"blogName"},"id=" + accountID,null,null,null,null);
  c.moveToFirst();
  if (c.getString(0) != null) {
    accountName=c.getString(0);
  }
  c.close();
  db.close();
  return accountName;
}
