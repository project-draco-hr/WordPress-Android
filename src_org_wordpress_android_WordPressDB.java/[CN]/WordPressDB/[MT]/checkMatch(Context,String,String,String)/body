{
  Cursor c=db.query(SETTINGS_TABLE,new String[]{"blogName","url"},"blogName='" + addSlashes(blogName) + "' AND url='"+ addSlashes(blogURL)+ "'"+ " AND username='"+ username+ "'",null,null,null,null);
  int numRows=c.getCount();
  boolean result=false;
  if (numRows > 0) {
    result=true;
  }
  c.close();
  return result;
}
