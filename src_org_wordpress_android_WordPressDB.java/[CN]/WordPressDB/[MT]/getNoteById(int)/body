{
  Cursor cursor=db.query(NOTES_TABLE,new String[]{"raw_note_data"},null,null,null,"id=" + id,null,null);
  cursor.moveToFirst();
  try {
    JSONObject jsonNote=new JSONObject(cursor.getString(0));
    return new Note(jsonNote);
  }
 catch (  JSONException e) {
    Log.e(WordPress.TAG,"Can't parse JSON Note: " + e);
    return null;
  }
}
