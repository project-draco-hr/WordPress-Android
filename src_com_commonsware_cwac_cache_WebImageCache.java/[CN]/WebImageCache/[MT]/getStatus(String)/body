{
  int result=super.getStatus(key);
  if (result == CACHE_NONE && getCacheRoot() != null) {
    try {
      File cache=buildCachedImagePath(key);
      if (cache.exists()) {
        result=CACHE_DISK;
      }
    }
 catch (    Throwable t) {
      Log.e(TAG,"Exception getting cache status",t);
    }
  }
  return (result);
}
