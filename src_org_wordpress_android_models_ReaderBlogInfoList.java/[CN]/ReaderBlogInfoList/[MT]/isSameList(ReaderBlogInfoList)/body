{
  if (blogs == null || blogs.size() != this.size()) {
    return false;
  }
  for (  ReaderBlogInfo blogInfo : blogs) {
    int index=indexOfBlogId(blogInfo.blogId);
    if (index == -1) {
      return false;
    }
    if (!this.get(index).isSameAs(blogInfo)) {
      return false;
    }
  }
  return true;
}
