{
  if (message != null) {
    if (message.contains(": "))     message=message.substring(message.indexOf(": ") + 2,message.length());
    if (message.contains("[code"))     message=message.substring(0,message.indexOf("[code"));
    message=StringUtils.unescapeHTML(message);
    return message;
  }
 else {
    return "";
  }
}
