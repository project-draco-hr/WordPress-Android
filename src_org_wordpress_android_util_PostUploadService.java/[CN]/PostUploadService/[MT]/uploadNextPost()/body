{
synchronized (listOfPosts) {
    if (currentTask == null) {
      currentUploadingPost=null;
      if (listOfPosts.size() > 0) {
        currentUploadingPost=listOfPosts.remove(0);
        currentTask=new UploadPostTask();
        currentTask.execute(currentUploadingPost);
      }
 else {
        this.stopSelf();
      }
    }
  }
}
