{
  mErrorMessage=String.format(context.getResources().getText(R.string.error_upload).toString(),post.isPage() ? context.getResources().getText(R.string.page).toString() : context.getResources().getText(R.string.post).toString()) + " " + e.getMessage();
  mIsMediaError=false;
  AppLog.e(T.EDITOR,mErrorMessage,e);
}
