{
  if (post == null)   return;
  int remoteBlogId=WordPress.wpDB.getRemoteBlogIdForLocalTableBlogId(post.getLocalTableBlogId());
  PostModel postModel=new PostModel(Integer.toString(remoteBlogId),post.getRemotePostId(),post.getTitle(),post.getLink(),isPage ? "page" : "post");
  viewStatsSinglePostDetails(context,postModel);
}
