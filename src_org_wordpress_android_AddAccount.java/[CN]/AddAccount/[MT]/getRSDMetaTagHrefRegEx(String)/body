{
  InputStream in=ApiHelper.getResponse(urlString);
  if (in != null) {
    try {
      String html=ApiHelper.convertStreamToString(in);
      Matcher matcher=rsdLink.matcher(html);
      if (matcher.find()) {
        String href=matcher.group(1);
        return href;
      }
    }
 catch (    IOException e) {
      Log.e("wp_android","IOEX",e);
      return null;
    }
  }
  return null;
}
