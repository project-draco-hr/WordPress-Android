{
  for (  Request<JSONObject> req : statsNetworkRequests) {
    if (req != null && !req.hasHadResponseDelivered() && !req.isCanceled()) {
      req.cancel();
    }
  }
  statsNetworkRequests.clear();
  if (orchestrator != null) {
    orchestrator.interrupt();
  }
  orchestrator=null;
  this.mCurrentBlogId=null;
}
