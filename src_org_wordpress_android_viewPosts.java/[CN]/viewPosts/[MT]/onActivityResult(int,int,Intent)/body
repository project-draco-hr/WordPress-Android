{
  super.onActivityResult(requestCode,resultCode,data);
  Bundle extras=data.getExtras();
  String returnResult=extras.getString("returnStatus");
  if (returnResult != null) {
switch (requestCode) {
case 0:
      if (returnResult.equals("OK")) {
        loadPosts();
      }
    break;
case 1:
  if (returnResult.equals("OK")) {
    refreshPosts();
  }
break;
}
}
}
