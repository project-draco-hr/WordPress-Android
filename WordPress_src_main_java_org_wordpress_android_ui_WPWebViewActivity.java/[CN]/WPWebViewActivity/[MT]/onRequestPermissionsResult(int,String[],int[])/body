{
switch (requestCode) {
case EXTERNAL_WRITE_STORAGE_REQUEST_CODE:
    for (    int grantResult : grantResults) {
      if (grantResult == PackageManager.PERMISSION_DENIED) {
        ToastUtils.showToast(this,"Permissions required in order to download export file");
        return;
      }
    }
  downloadURL();
break;
default :
break;
}
}
