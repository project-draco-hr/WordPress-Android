{
  if (mDefaultImageResId != 0) {
    setImageResource(mDefaultImageResId);
    return;
  }
switch (mImageType) {
case NONE:
    break;
case AVATAR:
  setImageResource(R.drawable.shape_oval_grey_light);
break;
default :
setImageDrawable(new ColorDrawable(getColorRes(R.color.grey_light)));
break;
}
}
