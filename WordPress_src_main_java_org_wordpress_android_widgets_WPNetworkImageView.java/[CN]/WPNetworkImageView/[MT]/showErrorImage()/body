{
switch (mImageType) {
case NONE:
    break;
case AVATAR:
  if (getContext() == null)   break;
setImageBitmap(ImageUtils.getCircularBitmap(BitmapFactory.decodeResource(getContext().getResources(),R.drawable.gravatar_placeholder)));
break;
default :
setImageDrawable(new ColorDrawable(getColorRes(R.color.grey_medium)));
break;
}
}
