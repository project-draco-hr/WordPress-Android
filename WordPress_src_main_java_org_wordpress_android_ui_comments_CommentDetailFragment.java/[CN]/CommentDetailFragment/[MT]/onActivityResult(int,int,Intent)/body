{
  super.onActivityResult(requestCode,resultCode,data);
  if (requestCode == Constants.INTENT_COMMENT_EDITOR && resultCode == Activity.RESULT_OK) {
    reloadComment();
    if (mOnCommentChangeListener != null)     mOnCommentChangeListener.onCommentChanged(ChangedFrom.COMMENT_DETAIL,ChangeType.EDITED);
  }
}
