{
  if (blogInfo == null) {
    return 0;
  }
  String updated=getBlogInfoLastUpdated(blogInfo);
  if (TextUtils.isEmpty(updated)) {
    return NEVER_UPDATED;
  }
  Date dtUpdated=DateTimeUtils.dateFromIso8601(updated);
  if (dtUpdated == null) {
    return 0;
  }
  Date dtNow=new Date();
  return DateTimeUtils.minutesBetween(dtUpdated,dtNow);
}
