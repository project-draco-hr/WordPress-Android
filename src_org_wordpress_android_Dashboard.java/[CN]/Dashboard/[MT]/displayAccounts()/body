{
  Vector<?> accounts=WordPress.wpDB.getAccounts(this);
  if (accounts.size() == 0) {
    Intent i=new Intent(Dashboard.this,NewAccount.class);
    startActivityForResult(i,0);
  }
 else {
    id=WordPress.getCurrentBlog(Dashboard.this).getId();
    setupDashboardButtons();
    updateCommentBadge();
  }
}
