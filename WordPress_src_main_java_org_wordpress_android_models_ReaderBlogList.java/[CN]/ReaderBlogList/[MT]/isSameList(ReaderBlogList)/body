{
  if (blogs == null || blogs.size() != this.size()) {
    return false;
  }
  for (  ReaderBlog blogInfo : blogs) {
    int index;
    if (blogInfo.feedId != 0) {
      index=indexOfFeedId(blogInfo.feedId);
    }
 else {
      index=indexOfBlogId(blogInfo.blogId);
    }
    if (index == -1 || !this.get(index).isSameAs(blogInfo)) {
      return false;
    }
  }
  return true;
}
