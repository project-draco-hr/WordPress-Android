{
  return post != null && post.blogId == this.blogId && post.postId == this.postId && post.numLikes == this.numLikes && post.numReplies == this.numReplies && post.isFollowedByCurrentUser == this.isFollowedByCurrentUser && post.isLikedByCurrentUser == this.isLikedByCurrentUser && post.isCommentsOpen == this.isCommentsOpen && post.isLikesEnabled == this.isLikesEnabled;
}
