{
  if (!isAdded()) {
    return;
  }
  if (url != null && url.equals("about:blank")) {
    refreshLikes();
    refreshCommentCount();
    if (!mHasAlreadyUpdatedPost) {
      mHasAlreadyUpdatedPost=true;
      updatePost();
    }
  }
 else {
    AppLog.w(T.READER,"reader post detail > page finished - " + url);
  }
}
