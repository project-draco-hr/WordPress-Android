{
  if (!isAdded() || TextUtils.isEmpty(imageUrl)) {
    return false;
  }
  if (!imageUrl.startsWith("http")) {
    return false;
  }
  boolean isPrivate=(mPost != null && mPost.isPrivate);
  if (isPrivate) {
    imageUrl=UrlUtils.makeHttps(imageUrl);
  }
  ReaderActivityLauncher.showReaderPhotoViewer(getActivity(),imageUrl,getPostContent(),source,mPost.isPrivate,startX,startY);
  return true;
}
