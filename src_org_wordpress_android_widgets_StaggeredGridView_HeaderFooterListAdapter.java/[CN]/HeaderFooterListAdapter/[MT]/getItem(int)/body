{
  if (position < headerSize) {
    return mHeaderView;
  }
  final int adjPosition=position - headerSize;
  int adapterCount=0;
  if (mAdapter != null) {
    adapterCount=mAdapter.getCount();
    if (adjPosition < adapterCount) {
      return mAdapter.getItem(adjPosition);
    }
  }
  return mFooterView;
}
