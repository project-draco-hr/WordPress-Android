{
  if (position < headerSize) {
    return HEADER_TYPE;
  }
  if (mAdapter != null && position >= headerSize) {
    int adjPosition=position - headerSize;
    int adapterCount=mAdapter.getCount();
    if (adjPosition < adapterCount) {
      return REFRESHABLE_LIST_TYPE;
    }
  }
  return FOOTER_TYPE;
}
