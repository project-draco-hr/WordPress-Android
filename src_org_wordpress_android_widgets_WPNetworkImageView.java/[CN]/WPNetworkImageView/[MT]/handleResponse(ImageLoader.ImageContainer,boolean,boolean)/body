{
  if (response.getBitmap() != null) {
    setImageBitmap(response.getBitmap());
    if (!isCached && allowFadeIn && (mImageType == ImageType.PHOTO || mImageType == ImageType.VIDEO))     fadeIn();
    if (mImageListener != null)     mImageListener.onImageLoaded(true);
  }
 else {
    showDefaultImage(mImageType);
  }
}
