{
switch (imageType) {
case AVATAR:
    if (mDefaultAvatar == null)     mDefaultAvatar=context.getResources().getDrawable(R.drawable.reader_avatar_default);
  return mDefaultAvatar;
case PHOTO_FULL:
return null;
default :
if (mDefaultPhoto == null) mDefaultPhoto=context.getResources().getDrawable(R.drawable.reader_photo_default);
return mDefaultPhoto;
}
}
