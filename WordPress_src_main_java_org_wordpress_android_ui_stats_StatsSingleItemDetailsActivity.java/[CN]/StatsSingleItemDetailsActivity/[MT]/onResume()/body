{
  super.onResume();
  if (mRestResponseParsed == null) {
    mHandler.postDelayed(new Runnable(){
      @Override public void run(){
        if (!isFinishing()) {
          setupEmptyGraph("");
          showHideEmptyModulesIndicator(true);
          refreshStats();
        }
      }
    }
,75L);
  }
 else {
    updateUI();
  }
  ActivityId.trackLastActivity(ActivityId.STATS_POST_DETAILS);
}
