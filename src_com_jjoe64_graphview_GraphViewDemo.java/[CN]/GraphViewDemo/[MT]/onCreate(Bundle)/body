{
  super.onCreate(savedInstanceState);
  LineGraphView graphView=new LineGraphView(this,"GraphViewDemo");
  graphView.addSeries(new GraphViewSeries(null,null,new GraphViewData[]{new GraphViewData(1,2.0d),new GraphViewData(2,1.5d),new GraphViewData(2.5,3.0d),new GraphViewData(3,2.5d),new GraphViewData(4,1.0d),new GraphViewData(5,3.0d)}));
  setContentView(graphView);
}
