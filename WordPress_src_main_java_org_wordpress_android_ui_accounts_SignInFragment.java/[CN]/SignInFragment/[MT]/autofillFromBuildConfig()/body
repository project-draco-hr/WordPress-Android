{
  if (!BuildConfig.DEBUG)   return;
  String userName=(String)WordPress.getBuildConfigValue(getActivity(),"DOTCOM_LOGIN_USERNAME");
  String password=(String)WordPress.getBuildConfigValue(getActivity(),"DOTCOM_LOGIN_PASSWORD");
  if (!TextUtils.isEmpty(userName)) {
    mUsernameEditText.setText(userName);
    AppLog.d(T.NUX,"Autofilled username from build config");
  }
  if (!TextUtils.isEmpty(password)) {
    mPasswordEditText.setText(password);
    AppLog.d(T.NUX,"Autofilled password from build config");
  }
}
