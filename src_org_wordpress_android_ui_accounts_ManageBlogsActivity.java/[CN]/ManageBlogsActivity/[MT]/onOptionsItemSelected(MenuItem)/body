{
  int itemId=item.getItemId();
switch (itemId) {
case R.id.menu_refresh:
    refreshBlogs();
  return true;
case R.id.menu_select_all:
selectAll();
return true;
case R.id.menu_deselect_all:
deselectAll();
return true;
case android.R.id.home:
finish();
return true;
}
return super.onOptionsItemSelected(item);
}
