{
  new AlertDialog.Builder(getActivity()).setMessage(getString(R.string.sign_out_confirm)).setPositiveButton(R.string.signout,new DialogInterface.OnClickListener(){
    public void onClick(    DialogInterface dialog,    int whichButton){
      signOutWordPressCom();
    }
  }
).setNegativeButton(R.string.cancel,null).setCancelable(true).create().show();
}
