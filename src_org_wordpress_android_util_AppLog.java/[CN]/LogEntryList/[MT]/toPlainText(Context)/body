{
  StringBuilder sb=new StringBuilder();
  sb.append("WordPress Android version: " + WordPress.getVersionName(context)).append("\n").append("Android device name: " + DeviceUtils.getInstance().getDeviceName(context)).append("\n\n");
  Iterator<LogEntry> it=mLogEntries.iterator();
  int lineNum=1;
  while (it.hasNext()) {
    sb.append(String.format("%02d - ",lineNum)).append(it.next().logText).append("\n");
    lineNum++;
  }
  return sb.toString();
}
