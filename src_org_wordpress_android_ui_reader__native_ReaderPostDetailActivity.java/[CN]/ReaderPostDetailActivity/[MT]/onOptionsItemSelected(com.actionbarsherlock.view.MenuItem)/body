{
switch (item.getItemId()) {
case android.R.id.home:
    onBackPressed();
  return true;
case R.id.menu_browse:
ReaderActivityLauncher.openUrl(this,mPost.getUrl(),OpenUrlType.EXTERNAL);
return true;
case R.id.menu_share:
sharePage();
return true;
default :
return super.onOptionsItemSelected(item);
}
}
