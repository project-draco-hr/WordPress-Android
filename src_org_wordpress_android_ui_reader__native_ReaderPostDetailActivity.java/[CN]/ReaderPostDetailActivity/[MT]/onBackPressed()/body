{
  if (mIsAddCommentBoxShowing) {
    hideAddCommentBox();
  }
 else {
    Intent data=new Intent();
    data.putExtra(ARG_BLOG_ID,mBlogId);
    data.putExtra(ARG_POST_ID,mPostId);
    if (mIsPostChanged) {
      setResult(RESULT_OK,data);
    }
 else {
      setResult(RESULT_CANCELED,data);
    }
    super.onBackPressed();
  }
}
