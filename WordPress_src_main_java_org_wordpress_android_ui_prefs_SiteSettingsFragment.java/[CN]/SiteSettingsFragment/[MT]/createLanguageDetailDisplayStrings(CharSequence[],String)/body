{
  if (languageCodes == null || languageCodes.length < 1)   return null;
  String[] detailStrings=new String[languageCodes.length];
  for (int i=0; i < languageCodes.length; ++i) {
    detailStrings[i]=StringUtils.capitalize(getLanguageString(languageCodes[i].toString(),WPPrefUtils.languageLocale(locale)));
  }
  return detailStrings;
}
