{
  getNumberPickerDialog(false,false,null,-1,R.string.multiple_links_description,-1,0,getResources().getInteger(R.integer.max_links_limit),mSiteSettings.getMultipleLinks(),R.string.site_settings_multiple_links_title,new DialogInterface.OnClickListener(){
    @Override public void onClick(    DialogInterface dialog,    int which){
      if (mSiteSettings.getMultipleLinks() != mNumberPicker.getValue()) {
        onPreferenceChange(mMultipleLinksPref,mNumberPicker.getValue());
      }
    }
  }
,null);
}
