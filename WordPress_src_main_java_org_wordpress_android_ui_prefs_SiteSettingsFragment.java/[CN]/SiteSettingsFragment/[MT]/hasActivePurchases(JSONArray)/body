{
  for (int i=0; i < purchases.length(); i++) {
    try {
      JSONObject purchase=purchases.getJSONObject(i);
      int active=purchase.getInt("active");
      if (active == 1) {
        return true;
      }
    }
 catch (    JSONException e) {
      e.printStackTrace();
    }
  }
  return false;
}
