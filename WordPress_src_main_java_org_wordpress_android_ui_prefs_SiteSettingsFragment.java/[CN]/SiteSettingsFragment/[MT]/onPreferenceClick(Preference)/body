{
  if (preference == mRelatedPostsPreference) {
    showRelatedPostsDialog();
    return true;
  }
 else   if (preference == mMultipleLinksPreference) {
    return true;
  }
 else   if (preference == mModerationHoldPreference) {
    showListEditorDialog(R.string.site_settings_moderation_hold_title,R.string.hold_for_moderation_description,mSiteSettings.getModerationKeys());
    mEditingList=mSiteSettings.getModerationKeys();
    showFab();
    return true;
  }
 else   if (preference == mBlacklistPreference) {
    showListEditorDialog(R.string.site_settings_blacklist_title,R.string.blacklist_description,mSiteSettings.getBlacklistKeys());
    mEditingList=mSiteSettings.getBlacklistKeys();
    showFab();
    return true;
  }
  return false;
}
