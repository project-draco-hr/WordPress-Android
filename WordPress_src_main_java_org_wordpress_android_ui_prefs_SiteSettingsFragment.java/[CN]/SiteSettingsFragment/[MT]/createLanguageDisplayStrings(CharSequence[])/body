{
  if (languageCodes == null || languageCodes.length < 1)   return null;
  String[] displayStrings=new String[languageCodes.length];
  for (int i=0; i < languageCodes.length; ++i) {
    displayStrings[i]=firstLetterCapitalized(getLanguageString(String.valueOf(languageCodes[i]),new Locale(localeInput(languageCodes[i].toString()))));
  }
  return displayStrings;
}
