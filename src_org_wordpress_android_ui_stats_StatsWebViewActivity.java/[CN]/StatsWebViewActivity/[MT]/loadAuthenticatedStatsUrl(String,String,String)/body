{
  try {
    String postData=String.format("log=%s&pwd=%s&redirect_to=%s",URLEncoder.encode(username,"UTF-8"),URLEncoder.encode(passwd,"UTF-8"),URLEncoder.encode(url,"UTF-8"));
    mWebView.postUrl("https://wordpress.com/wp-login.php",postData.getBytes());
  }
 catch (  UnsupportedEncodingException e) {
    AppLog.e(AppLog.T.STATS,e);
  }
}
