{
  SharedPreferences sharedPreferences=editText.getContext().getSharedPreferences(PREFERENCES_NAME,Context.MODE_PRIVATE);
  String text=sharedPreferences.getString(getViewPathId(editText) + mUniqueId,"");
  if (!text.isEmpty()) {
    editText.setText(text);
    editText.setSelection(text.length());
  }
}
