{
  if (posts == null || posts.size() == 0) {
    return false;
  }
  boolean isChanged=false;
  for (  ReaderPost post : posts) {
    boolean isFollowed=isPostFollowed(post);
    if (isFollowed != post.isFollowedByCurrentUser) {
      post.isFollowedByCurrentUser=isFollowed;
      isChanged=true;
    }
  }
  return isChanged;
}
