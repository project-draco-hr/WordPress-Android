{
  ReaderPostList posts=new ReaderPostList();
  try {
    if (cursor != null && cursor.moveToFirst()) {
      do {
        posts.add(getPostFromCursor(cursor));
      }
 while (cursor.moveToNext());
    }
  }
 catch (  IllegalStateException e) {
    AppLog.e(AppLog.T.READER,e);
  }
  return posts;
}
