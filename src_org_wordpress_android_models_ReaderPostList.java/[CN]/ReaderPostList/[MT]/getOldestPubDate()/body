{
  Date oldestDate=null;
  for (  ReaderPost post : this) {
    Date dtPublished=post.getDatePublished();
    if (dtPublished != null) {
      if (oldestDate == null) {
        oldestDate=dtPublished;
      }
 else       if (oldestDate.after(dtPublished)) {
        oldestDate=dtPublished;
      }
    }
  }
  return oldestDate;
}
