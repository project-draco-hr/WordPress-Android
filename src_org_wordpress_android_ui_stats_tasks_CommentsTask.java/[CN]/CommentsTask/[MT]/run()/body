{
  WordPress.restClient.getStatsMostCommented(mBlogId,new RestRequest.Listener(){
    @Override public void onResponse(    JSONObject response){
      parseMostCommentedResponse(response);
    }
  }
,new RestRequest.ErrorListener(){
    @Override public void onErrorResponse(    VolleyError error){
      AppLog.e(AppLog.T.STATS,error);
    }
  }
);
  WordPress.restClient.getStatsTopCommenters(mBlogId,new RestRequest.Listener(){
    @Override public void onResponse(    JSONObject response){
      parseTopCommenterResponse(response);
    }
  }
,new RestRequest.ErrorListener(){
    @Override public void onErrorResponse(    VolleyError error){
      AppLog.e(AppLog.T.STATS,error);
    }
  }
);
}
