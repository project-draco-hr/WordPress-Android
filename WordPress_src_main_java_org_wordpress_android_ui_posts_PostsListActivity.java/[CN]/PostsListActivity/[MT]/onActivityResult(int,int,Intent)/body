{
  if (requestCode == RequestCodes.EDIT_POST && resultCode == RESULT_OK && data != null && data.getBooleanExtra(EditPostActivity.EXTRA_SHOULD_REFRESH,false)) {
    mPostList.getPostListAdapter().loadPosts();
  }
 else   if (requestCode == RequestCodes.PREVIEW_POST) {
    mPostList.getPostListAdapter().loadPosts();
  }
  super.onActivityResult(requestCode,resultCode,data);
}
