{
  mOauth.requestAccessToken(username,password,new OauthTokenResponseHandler(){
    @Override public void onStart(){
    }
    /** 
 * Save the token to a preference
 */
    @Override public void onSuccess(    OauthToken token){
    }
    @Override public void onFailure(    Throwable e,    JSONObject respose){
    }
    @Override public void onFinish(){
    }
  }
);
}
