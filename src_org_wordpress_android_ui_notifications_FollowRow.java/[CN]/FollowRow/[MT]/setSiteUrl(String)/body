{
  if (url != null) {
    mBlogURL=url;
    this.setOnClickListener(new OnClickListener(){
      @Override public void onClick(      View v){
        if (mBlogURL != null) {
          try {
            Uri uri=Uri.parse(mBlogURL);
            getContext().startActivity(new Intent(Intent.ACTION_VIEW,uri));
          }
 catch (          Exception e) {
            e.printStackTrace();
          }
        }
      }
    }
);
  }
}
