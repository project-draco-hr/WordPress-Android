{
  if (isFinishing()) {
    return;
  }
  final Post post=WordPress.wpDB.getPostForLocalTablePostId(mLocalPostId);
  if (post == null) {
    ToastUtils.showToast(this,R.string.post_not_found);
    finish();
    return;
  }
  mWebView.loadDataWithBaseURL(null,formatPostContentForWebView(post),"text/html","utf-8",null);
}
