{
switch (mEmptyViewMessage) {
case LOADING:
    mEmptyViewTitle.setText(mIsPage ? org.wordpress.android.R.string.pages_fetching : org.wordpress.android.R.string.posts_fetching);
  break;
case NO_CONTENT:
mEmptyViewTitle.setText(mIsPage ? org.wordpress.android.R.string.pages_empty_list : org.wordpress.android.R.string.posts_empty_list);
setRefreshing(false);
mKeepSwipeRefreshLayoutVisible=false;
break;
default :
break;
}
}
