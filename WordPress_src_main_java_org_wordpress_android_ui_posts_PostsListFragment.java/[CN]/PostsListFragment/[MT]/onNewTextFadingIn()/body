{
switch (mEmptyViewMessage) {
case LOADING:
    mEmptyViewTitle.setText(mIsPage ? org.wordpress.android.R.string.fetching_pages : org.wordpress.android.R.string.fetching_posts);
  break;
case NO_CONTENT:
mEmptyViewTitle.setText(mIsPage ? org.wordpress.android.R.string.pages_empty_list : org.wordpress.android.R.string.posts_empty_list);
mSwipeToRefreshHelper.setRefreshing(false);
mKeepSwipeRefreshLayoutVisible=false;
break;
default :
break;
}
}
