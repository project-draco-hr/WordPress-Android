{
  if (!isAdded()) {
    return;
  }
  if (WordPress.getCurrentBlog() == null || WordPress.getCurrentBlog().getLocalTableBlogId() != event.mLocalId) {
    return;
  }
  setRefreshing(false);
  if (!NetworkUtils.isNetworkAvailable(getActivity())) {
    updateEmptyView(EmptyViewMessageType.NETWORK_ERROR);
    return;
  }
  loadPosts();
}
