{
  String patchedCode=deviceLanguageCode;
  if (deviceLanguageCode != null) {
    if (deviceLanguageCode.startsWith("iw"))     patchedCode=deviceLanguageCode.replace("iw","he");
 else     if (deviceLanguageCode.startsWith("in"))     patchedCode=deviceLanguageCode.replace("in","id");
 else     if (deviceLanguageCode.startsWith("ji"))     patchedCode=deviceLanguageCode.replace("ji","yi");
  }
  return patchedCode;
}
