{
  if (mIsCompleted)   return;
  AppLog.d(AppLog.T.STATS,"waiting for " + getTaskName());
  while (!mIsCompleted) {
    try {
      Thread.sleep(1000);
    }
 catch (    InterruptedException e) {
      AppLog.e(AppLog.T.STATS,e);
      return;
    }
  }
  AppLog.d(AppLog.T.STATS,"completed " + getTaskName());
}
