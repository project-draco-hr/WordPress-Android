{
  SharedPreferences prefs=PreferenceManager.getDefaultSharedPreferences(WordPress.getContext());
  mHasUserOptedOut=!prefs.getBoolean("wp_pref_send_usage_stats",true);
  if (manageSession) {
    if (mHasUserOptedOut) {
      endSession(true);
      clearAllData();
    }
 else {
      beginSession();
    }
  }
}
