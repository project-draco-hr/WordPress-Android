{
  if (view instanceof TextView) {
    Typeface type=TypefaceCache.getTypeface(getContext(),TypefaceCache.FAMILY_OPEN_SANS,Typeface.NORMAL,TypefaceCache.VARIATION_NORMAL);
    ((TextView)view).setTypeface(type);
    ((TextView)view).setTextSize(24);
    ((TextView)view).setTextColor(getResources().getColor(R.color.wp_blue));
  }
}
