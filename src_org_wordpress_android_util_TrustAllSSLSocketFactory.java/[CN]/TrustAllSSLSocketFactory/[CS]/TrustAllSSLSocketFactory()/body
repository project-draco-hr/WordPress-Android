{
  super((KeyStore)null);
  try {
    SSLContext sslcontext=SSLContext.getInstance("TLS");
    sslcontext.init(null,new TrustManager[]{new TrustAllManager()},null);
    factory=sslcontext.getSocketFactory();
    setHostnameVerifier(new AllowAllHostnameVerifier());
  }
 catch (  Exception ex) {
  }
}
