{
  if (!(WPActionBarActivity.this instanceof StatsActivity || WPActionBarActivity.this instanceof StatsActivityTablet))   mShouldFinish=true;
  Intent intent;
  if (Utils.isTablet(WPActionBarActivity.this))   intent=new Intent(WPActionBarActivity.this,StatsActivityTablet.class);
 else   intent=new Intent(WPActionBarActivity.this,StatsActivity.class);
  intent.putExtra("id",WordPress.currentBlog.getId());
  intent.putExtra("isNew",true);
  intent.setFlags(Intent.FLAG_ACTIVITY_NO_ANIMATION);
  startActivityWithDelay(intent);
}
