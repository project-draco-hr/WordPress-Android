{
  if (!(WPActionBarActivity.this instanceof PostsActivity) || (WPActionBarActivity.this instanceof PagesActivity)) {
    mShouldFinish=true;
    WPStats.track(WPStats.Stat.OPENED_POSTS);
  }
  Intent intent=new Intent(WPActionBarActivity.this,PostsActivity.class);
  intent.setFlags(Intent.FLAG_ACTIVITY_NO_ANIMATION);
  startActivityWithDelay(intent);
}
