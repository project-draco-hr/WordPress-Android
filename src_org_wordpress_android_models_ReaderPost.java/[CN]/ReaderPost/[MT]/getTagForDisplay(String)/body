{
  if (tagForDisplay == null) {
    if (!isPrivate && hasPrimaryTag()) {
      if (getPrimaryTag().equalsIgnoreCase(currentTag)) {
        tagForDisplay=getSecondaryTag();
      }
 else {
        tagForDisplay=getPrimaryTag();
      }
    }
 else {
      tagForDisplay="";
    }
  }
  return tagForDisplay;
}
