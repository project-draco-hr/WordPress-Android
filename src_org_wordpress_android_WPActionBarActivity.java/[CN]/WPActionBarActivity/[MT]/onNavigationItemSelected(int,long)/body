{
  if (blogIDs[pos] == -1) {
    Intent i=new Intent(this,NewAccount.class);
    startActivityForResult(i,0);
  }
 else {
    try {
      WordPress.currentBlog=new Blog(blogIDs[pos],this);
      WordPress.wpDB.updateLastBlogId(blogIDs[pos]);
      onBlogChanged();
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
  return true;
}
