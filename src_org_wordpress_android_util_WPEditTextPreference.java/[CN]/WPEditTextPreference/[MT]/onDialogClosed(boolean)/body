{
  super.onDialogClosed(positiveResult);
  if (positiveResult) {
    if (getEditText() != null && getEditText().getText() != null) {
      String value=getEditText().getText().toString();
      if (callChangeListener(value)) {
        setText(value);
      }
    }
  }
 else {
    callChangeListener(null);
  }
}
