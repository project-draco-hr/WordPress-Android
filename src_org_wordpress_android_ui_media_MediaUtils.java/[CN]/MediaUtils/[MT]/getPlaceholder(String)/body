{
  if (isValidImage(url))   return R.drawable.media_image_placeholder;
 else   if (isDocument(url))   return R.drawable.media_document;
 else   if (isPowerpoint(url))   return R.drawable.media_powerpoint;
 else   if (isSpreadsheet(url))   return R.drawable.media_spreadsheet;
 else   if (isVideo(url))   return R.drawable.media_movieclip;
  return 0;
}
