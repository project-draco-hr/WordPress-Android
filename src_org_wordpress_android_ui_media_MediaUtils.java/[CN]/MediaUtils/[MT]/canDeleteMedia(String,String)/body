{
  Cursor cursor=WordPress.wpDB.getMediaFile(blogId,mediaID);
  if (!cursor.moveToFirst()) {
    cursor.close();
    return false;
  }
  String state=cursor.getString(cursor.getColumnIndex("uploadState"));
  cursor.close();
  if (state != null && state.equals("uploading")) {
    return false;
  }
  return true;
}
