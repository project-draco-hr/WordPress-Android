{
  CommentView cv;
  if (convertView == null) {
    cv=new CommentView(mContext,postIDs[position],titles[position],publish[position]);
  }
 else {
    cv=(CommentView)convertView;
    cv.setId(postIDs[position]);
    cv.setTitle(titles[position]);
    cv.setPublished(publish[position]);
  }
  return cv;
}
