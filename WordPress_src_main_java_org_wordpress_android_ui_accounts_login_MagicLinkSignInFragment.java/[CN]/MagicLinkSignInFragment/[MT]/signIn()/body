{
  if (mSelfHosted || isEnterPasswordMode()) {
    super.signIn();
  }
 else {
    if (isUsernameEmail()) {
      boolean isValidWPComEmail=false;
      if (isValidWPComEmail) {
        mListener.onMagicLinkRequestSuccess();
      }
 else {
        showPasswordFieldAndFocus();
      }
    }
 else {
      showPasswordFieldAndFocus();
    }
  }
}
