{
switch (item.getItemId()) {
case 0:
    launchPictureLibrary();
  return true;
case 1:
launchCamera();
return true;
case 2:
launchVideoLibrary();
return true;
case 3:
launchVideoCamera();
return true;
case 4:
startMediaGalleryActivity(null);
return true;
case 5:
startMediaGalleryAddActivity();
return true;
}
return false;
}
