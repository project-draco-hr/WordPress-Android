{
  FragmentManager fm=getSupportFragmentManager();
  if (mMenuDrawer.isShown()) {
    super.onBackPressed();
  }
 else   if (isInMultiSelect()) {
    cancelMultiSelect();
  }
 else   if (fm.getBackStackEntryCount() > 0) {
    fm.popBackStack();
    setupBaseLayout();
  }
 else {
    super.onBackPressed();
  }
}
