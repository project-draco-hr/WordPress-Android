{
  FragmentManager fm=getSupportFragmentManager();
  ViewPostFragment f=(ViewPostFragment)fm.findFragmentById(R.id.mediaItemFragment);
  if (!mSpinner.isShown()) {
    mSpinner.setVisibility(View.VISIBLE);
  }
  if (f == null) {
    try {
      fm.popBackStack();
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
}
