{
  Cursor c=ReaderDatabase.getReadableDb().rawQuery("SELECT blog_url FROM tbl_blog_urls WHERE is_followed!=0",null);
  try {
    ReaderUrlList urls=new ReaderUrlList();
    if (c.moveToFirst()) {
      do {
        urls.add(c.getString(0));
      }
 while (c.moveToNext());
    }
    return urls;
  }
  finally {
    SqlUtils.closeCursor(c);
  }
}
