{
  Cursor c=ReaderDatabase.getReadableDb().rawQuery("SELECT blog_id FROM tbl_ignored_recommendations",null);
  try {
    ArrayList<Long> blogIds=new ArrayList<Long>();
    if (c.moveToFirst()) {
      do {
        blogIds.add(c.getLong(0));
      }
 while (c.moveToNext());
    }
    return blogIds;
  }
  finally {
    SqlUtils.closeCursor(c);
  }
}
