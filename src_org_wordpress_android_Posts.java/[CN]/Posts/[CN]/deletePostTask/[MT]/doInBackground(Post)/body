{
  boolean result=false;
  post=params[0];
  XMLRPCClient client=new XMLRPCClient(WordPress.currentBlog.getUrl(),WordPress.currentBlog.getHttpuser(),WordPress.currentBlog.getHttppassword(),Posts.this);
  Object[] postParams={"",post.getPostid(),WordPress.currentBlog.getUsername(),WordPress.currentBlog.getPassword()};
  Object[] pageParams={WordPress.currentBlog.getBlogId(),WordPress.currentBlog.getUsername(),WordPress.currentBlog.getPassword(),post.getPostid()};
  try {
    client.call((isPage) ? "wp.deletePage" : "blogger.deletePost",(isPage) ? pageParams : postParams);
    result=true;
  }
 catch (  final XMLRPCException e) {
    errorMsg=e.getLocalizedMessage();
    result=false;
  }
  return result;
}
