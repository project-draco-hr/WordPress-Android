{
  String html=getHTML(urlString);
  if (html != "") {
    try {
      int location=html.indexOf("http://wp.me");
      String shortlink=html.substring(location,location + 30);
      shortlink=shortlink.substring(0,shortlink.indexOf("'"));
      return shortlink;
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
  return null;
}
