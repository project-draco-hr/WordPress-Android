{
  super.onNewIntent(intent);
  Bundle extras=intent.getExtras();
  if (extras != null) {
    isPage=extras.getBoolean("viewPages");
    postList.isPage=isPage;
  }
 else {
    isPage=false;
    postList.isPage=isPage;
  }
  titleBar.refreshBlog();
  attemptToSelectPost();
  postList.loadPosts(false);
}
