{
  if (date == null) {
    AppLog.w(T.UTILS,"Trying to parse a null date");
    return -1;
  }
  if (pattern == null) {
    AppLog.w(T.UTILS,"Trying to parse with a null pattern");
    return -1;
  }
  SimpleDateFormat sdf=new SimpleDateFormat(pattern);
  try {
    return sdf.parse(date).getTime();
  }
 catch (  ParseException e) {
    AppLog.e(T.UTILS,e);
  }
  return -1;
}
