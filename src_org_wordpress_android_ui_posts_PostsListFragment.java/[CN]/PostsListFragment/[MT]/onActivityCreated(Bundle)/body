{
  super.onActivityCreated(bundle);
  TextView textview=(TextView)getListView().getEmptyView();
  if (textview != null) {
    if (isPage) {
      textview.setText(getText(R.string.pages_empty_list));
    }
 else {
      textview.setText(getText(R.string.posts_empty_list));
    }
  }
  createSwitcher();
}
