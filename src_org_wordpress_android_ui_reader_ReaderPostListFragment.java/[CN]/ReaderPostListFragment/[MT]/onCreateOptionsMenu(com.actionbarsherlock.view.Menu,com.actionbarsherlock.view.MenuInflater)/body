{
  super.onCreateOptionsMenu(menu,inflater);
switch (getPostListType()) {
case TAG:
    if (isTagPreview()) {
      inflater.inflate(R.menu.basic_menu,menu);
    }
 else {
      inflater.inflate(R.menu.reader_native,menu);
      checkActionBar();
    }
  break;
case BLOG:
inflater.inflate(R.menu.basic_menu,menu);
break;
}
}
