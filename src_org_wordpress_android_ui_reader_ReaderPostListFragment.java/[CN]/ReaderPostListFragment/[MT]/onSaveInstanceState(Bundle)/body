{
  super.onSaveInstanceState(outState);
  AppLog.d(T.READER,"reader post list > saving instance state");
  if (hasCurrentTag()) {
    outState.putString(KEY_TAG_NAME,mCurrentTag);
  }
  if (mCurrentBlogId != 0) {
    outState.putLong(KEY_BLOG_ID,mCurrentBlogId);
  }
  if (mListView != null && mListView.getFirstVisiblePosition() > 0)   outState.putParcelable(KEY_LIST_STATE,mListView.onSaveInstanceState());
}
