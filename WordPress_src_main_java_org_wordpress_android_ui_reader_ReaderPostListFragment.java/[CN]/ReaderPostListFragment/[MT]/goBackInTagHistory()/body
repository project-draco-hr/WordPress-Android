{
  if (mTagPreviewHistory.empty()) {
    return false;
  }
  String tagName=mTagPreviewHistory.pop();
  if (isCurrentTagName(tagName)) {
    if (mTagPreviewHistory.empty()) {
      return false;
    }
    tagName=mTagPreviewHistory.pop();
  }
  setCurrentTag(new ReaderTag(tagName,ReaderTagType.FOLLOWED),false);
  updateFollowButton();
  return true;
}
