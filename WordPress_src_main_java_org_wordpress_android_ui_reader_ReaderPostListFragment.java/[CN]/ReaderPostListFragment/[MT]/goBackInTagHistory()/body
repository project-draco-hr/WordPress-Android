{
  if (mTagPreviewHistory.empty()) {
    return false;
  }
  String tag=mTagPreviewHistory.pop();
  if (isCurrentTagName(tag)) {
    if (mTagPreviewHistory.empty()) {
      return false;
    }
    tag=mTagPreviewHistory.pop();
  }
  setCurrentTagName(tag,false);
  updateFollowButton();
  return true;
}
