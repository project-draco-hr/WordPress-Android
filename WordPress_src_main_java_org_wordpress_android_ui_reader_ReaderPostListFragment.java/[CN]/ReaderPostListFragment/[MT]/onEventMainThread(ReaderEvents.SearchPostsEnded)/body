{
  if (!isAdded())   return;
  if (getPostListType() != ReaderPostListType.SEARCH_RESULTS || !event.getQuery().equals(mCurrentSearchQuery)) {
    return;
  }
  setIsUpdating(false);
  if (event.hasResults()) {
    getPostAdapter().setCurrentSearchQuery(event.getQuery());
  }
 else {
    mDataLoadedListener.onDataLoaded(true);
  }
}
