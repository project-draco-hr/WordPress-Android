{
  if (blogInfo == null) {
    return false;
  }
  if (this.numSubscribers != blogInfo.numSubscribers) {
    return false;
  }
  if (this.isFollowing != blogInfo.isFollowing) {
    return false;
  }
  if (!this.hasName() && blogInfo.hasName()) {
    return false;
  }
  if (!this.hasDescription() && blogInfo.hasDescription()) {
    return false;
  }
  return true;
}
