{
  return blogInfo != null && this.blogId == blogInfo.blogId && this.isFollowing == blogInfo.isFollowing && this.isPrivate == blogInfo.isPrivate && this.numSubscribers == blogInfo.numSubscribers && this.getName().equals(blogInfo.getName()) && this.getDescription().equals(blogInfo.getDescription()) && this.getUrl().equals(blogInfo.getUrl());
}
