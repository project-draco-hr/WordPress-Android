{
  mBlog=blog[0];
  XMLRPCClientInterface client=XMLRPCFactory.instantiate(mBlog.getUri(),mBlog.getHttpuser(),mBlog.getHttppassword());
  Object result=null;
  Object[] params={mBlog.getRemoteBlogId(),mBlog.getUsername(),mBlog.getPassword(),"show-supported"};
  try {
    result=client.call(ApiHelper.Methods.GET_POST_FORMATS,params);
  }
 catch (  ClassCastException cce) {
    setError(ErrorType.INVALID_RESULT,cce.getMessage(),cce);
  }
catch (  XMLRPCException e) {
    setError(ErrorType.NETWORK_XMLRPC,e.getMessage(),e);
  }
catch (  IOException e) {
    setError(ErrorType.NETWORK_XMLRPC,e.getMessage(),e);
  }
catch (  XmlPullParserException e) {
    setError(ErrorType.NETWORK_XMLRPC,e.getMessage(),e);
  }
  return result;
}
