{
  Cursor c=getReadableDatabase().query(PERSISTENTEDITTEXT_TABLE,new String[]{"value"},"key=?",new String[]{key},null,null,null);
  try {
    if (c.moveToFirst()) {
      return c.getString(0);
    }
  }
  finally {
    if (c != null && !c.isClosed()) {
      c.close();
    }
  }
  return defaultResult;
}
