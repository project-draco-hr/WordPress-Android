{
  if (plans == null) {
    AppLog.w(AppLog.T.PLANS,"The passed plans list is null!!");
    return null;
  }
  for (  Plan currentPlan : plans) {
    if (currentPlan.getProductID() == planID) {
      return currentPlan;
    }
  }
  AppLog.w(AppLog.T.PLANS,"Plan with ID " + planID + " wasn't found in the plans list");
  return null;
}
