{
  Blog blog=WordPress.getCurrentBlog();
  if (blog != null) {
    mLoading=true;
    notifyLoadingStatus();
    Cursor imageCursor=WordPressMediaUtils.getWordPressMediaImages(String.valueOf(blog.getLocalTableBlogId()));
    if (imageCursor != null) {
      addWordPressImagesFromCursor(imageCursor);
      imageCursor.close();
    }
  }
}
