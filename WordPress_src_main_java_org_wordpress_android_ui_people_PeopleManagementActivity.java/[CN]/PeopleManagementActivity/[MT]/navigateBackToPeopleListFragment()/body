{
  if (getFragmentManager().getBackStackEntryCount() > 0) {
    getFragmentManager().popBackStack();
    ActionBar actionBar=getSupportActionBar();
    if (actionBar != null) {
      actionBar.setElevation(getResources().getDimension(R.dimen.appbar_elevation));
    }
    return true;
  }
  return false;
}
