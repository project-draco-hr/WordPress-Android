{
  if (html != null) {
    html=html.replace("\\","\\\\");
    html=html.replace("\"","\\\"");
    html=html.replace("'","\\'");
    html=html.replace("\r","\\r");
    html=html.replace("\n","\\n");
    html=html.replace("\u2028","\\u2028");
    html=html.replace("\u2029","\\u2029");
  }
  return html;
}
