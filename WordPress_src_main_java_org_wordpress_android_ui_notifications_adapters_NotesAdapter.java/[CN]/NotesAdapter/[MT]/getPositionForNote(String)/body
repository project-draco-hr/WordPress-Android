{
  Bucket.ObjectCursor<Note> cursor=(Bucket.ObjectCursor<Note>)getCursor();
  if (cursor != null) {
    for (int i=0; i < cursor.getCount(); i++) {
      cursor.moveToPosition(i);
      String noteKey=cursor.getSimperiumKey();
      if (noteKey != null && noteKey.equals(noteId)) {
        return i;
      }
    }
  }
  return -1;
}
