{
  if (TextUtils.isEmpty(fullUrl))   return null;
  if (ReaderVideoUtils.isYouTubeVideoLink(fullUrl))   return ReaderVideoUtils.getYouTubeThumbnailUrl(fullUrl);
  return SqlUtils.stringForQuery(ReaderDatabase.getReadableDb(),"SELECT thumbnail_url FROM tbl_thumbnails WHERE full_url=?",new String[]{fullUrl});
}
