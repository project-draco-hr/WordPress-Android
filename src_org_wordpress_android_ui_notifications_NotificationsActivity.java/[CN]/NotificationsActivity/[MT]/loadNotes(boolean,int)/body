{
  new Thread(){
    @Override public void run(){
      final List<Note> notes=WordPress.wpDB.getLatestNotes();
      NotificationsActivity.this.runOnUiThread(new Runnable(){
        @Override public void run(){
          refreshNotificationsListFragment(notes);
          if (launchWithNoteId) {
            launchWithNoteId(noteId);
          }
        }
      }
);
    }
  }
.start();
}
