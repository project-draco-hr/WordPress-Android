{
  restClient.markNotificationsSeen(timestamp,new JsonHttpResponseHandler(){
    @Override public void onSuccess(    int status,    JSONObject response){
      Log.d(TAG,String.format("Set last seen time %s",response));
    }
    @Override public void onFailure(    Throwable e,    JSONObject response){
      Log.d(TAG,String.format("Failed to set last seen time %s",response),e);
    }
    @Override public void onFailure(    Throwable e,    JSONArray response){
      Log.d(TAG,String.format("Failed to set last seen time %s",response),e);
    }
    @Override public void onFailure(    Throwable e,    String response){
      Log.d(TAG,String.format("Failed to set last seen time %s",response),e);
    }
    @Override public void onFailure(    Throwable e){
      Log.d(TAG,"Failed to set last seen time %s",e);
    }
  }
);
}
