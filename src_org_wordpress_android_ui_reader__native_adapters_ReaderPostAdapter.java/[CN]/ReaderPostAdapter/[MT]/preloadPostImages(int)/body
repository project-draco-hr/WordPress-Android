{
  if (position >= mPosts.size() || position < 0) {
    ReaderLog.w("invalid preload position > " + Integer.toString(position));
    return;
  }
  ReaderPost post=mPosts.get(position);
  if (post.hasFeaturedImage())   preloadImage(post.getFeaturedImageForDisplay(mPhotonWidth,mPhotonHeight),mPhotonWidth,mPhotonHeight);
  if (post.hasPostAvatar())   preloadImage(post.getPostAvatarForDisplay(mAvatarSz),mAvatarSz,mAvatarSz);
  mLastPreloadPos=position;
}
