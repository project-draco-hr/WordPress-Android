{
  if (tagList == null || tagList.size() == 0) {
    return;
  }
  SQLiteStatement stmt=null;
  try {
    stmt=ReaderDatabase.getWritableDb().compileStatement("INSERT OR REPLACE INTO tbl_tags (tag_slug, tag_title, tag_type, endpoint) VALUES (?1,?2,?3,?4)");
    for (    ReaderTag tag : tagList) {
      stmt.bindString(1,tag.getTagSlug());
      stmt.bindString(2,tag.getTagTitle());
      stmt.bindLong(3,tag.tagType.toInt());
      stmt.bindString(4,tag.getEndpoint());
      stmt.execute();
    }
  }
  finally {
    SqlUtils.closeStatement(stmt);
  }
}
