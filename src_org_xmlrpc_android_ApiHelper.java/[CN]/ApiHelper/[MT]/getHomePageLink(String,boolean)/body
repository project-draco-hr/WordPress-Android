{
  Pattern xmlrpcLink=Pattern.compile("<homePageLink>(.*?)</homePageLink>",Pattern.CASE_INSENSITIVE | Pattern.DOTALL);
  String html=getResponse(urlString,trustAllSslCertificates);
  if (html != null) {
    Matcher matcher=xmlrpcLink.matcher(html);
    if (matcher.find()) {
      return matcher.group(1);
    }
  }
  return null;
}
