{
  HttpRequest request=getHttpRequest(urlString);
  if (request != null) {
    try {
      String body=request.body();
      return body;
    }
 catch (    HttpRequestException e) {
      Log.e("ApiHelper","Cannot load the content of " + urlString,e);
      return null;
    }
  }
 else {
    return null;
  }
}
