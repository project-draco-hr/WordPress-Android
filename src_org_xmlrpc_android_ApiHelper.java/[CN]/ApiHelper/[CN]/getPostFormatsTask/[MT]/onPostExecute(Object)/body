{
  try {
    HashMap<?,?> postFormats=(HashMap<?,?>)result;
    if (postFormats.size() > 0) {
      JSONObject jsonPostFormats=new JSONObject(postFormats);
      blog.setPostFormats(jsonPostFormats.toString());
      blog.save(ctx,null);
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
