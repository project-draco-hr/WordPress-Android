{
  try {
    HashMap<?,?> postFormats=(HashMap<?,?>)result;
    if (postFormats.size() > 0) {
      Gson gson=new Gson();
      String postFormatsJson=gson.toJson(postFormats);
      if (postFormatsJson != null) {
        blog.setPostFormats(postFormatsJson);
        blog.save(ctx,null);
      }
    }
  }
 catch (  Exception e) {
  }
}
