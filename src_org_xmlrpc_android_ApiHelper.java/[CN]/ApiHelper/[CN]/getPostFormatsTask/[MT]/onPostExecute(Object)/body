{
  try {
    Map<?,?> postFormats=(HashMap<?,?>)result;
    if (postFormats.size() > 0) {
      Gson gson=new Gson();
      String postFormatsJson=gson.toJson(postFormats);
      if (postFormatsJson != null) {
        if (blog.bsetPostFormats(postFormatsJson)) {
          blog.save();
        }
      }
    }
  }
 catch (  Exception e) {
  }
}
