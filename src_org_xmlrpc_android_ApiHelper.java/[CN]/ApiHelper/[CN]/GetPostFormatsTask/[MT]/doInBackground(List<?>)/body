{
  List<?> arguments=args[0];
  mBlog=(Blog)arguments.get(0);
  client=new XMLRPCClient(mBlog.getUrl(),mBlog.getHttpuser(),mBlog.getHttppassword());
  ApiHelper.addAuthorizationHeaderIfNeeded(mBlog,client);
  Object result=null;
  Object[] params={mBlog.getRemoteBlogId(),mBlog.getUsername(),mBlog.getPassword(),"show-supported"};
  try {
    result=client.call("wp.getPostFormats",params);
  }
 catch (  ClassCastException cce) {
    setError(ErrorType.INVALID_RESULT,cce.getMessage(),cce);
  }
catch (  XMLRPCException e) {
    setError(ErrorType.NETWORK_XMLRPC,e.getMessage(),e);
  }
  return result;
}
