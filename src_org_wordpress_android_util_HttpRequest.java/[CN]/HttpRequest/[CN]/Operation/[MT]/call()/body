{
  boolean thrown=false;
  try {
    return run();
  }
 catch (  HttpRequestException e) {
    thrown=true;
    throw e;
  }
catch (  IOException e) {
    thrown=true;
    throw new HttpRequestException(e);
  }
 finally {
    try {
      done();
    }
 catch (    IOException e) {
      if (!thrown)       throw new HttpRequestException(e);
    }
  }
}
