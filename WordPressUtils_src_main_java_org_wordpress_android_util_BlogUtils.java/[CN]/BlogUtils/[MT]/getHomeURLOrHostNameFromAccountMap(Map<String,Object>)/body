{
  String homeURL=MapUtils.getMapStr(account,"homeURL").replace("http://","").replace("https://","").trim();
  if (homeURL.endsWith("/")) {
    homeURL=homeURL.substring(0,homeURL.length() - 1);
  }
  if (homeURL.length() == 0) {
    return StringUtils.getHost(MapUtils.getMapStr(account,"url"));
  }
  return homeURL;
}
