{
  if (posts == null || posts.size() != this.size()) {
    return false;
  }
  for (  ReaderPost post : posts) {
    int index=indexOfPost(post.blogId,post.postId);
    if (index == -1) {
      return false;
    }
    ReaderPost thisPost=this.get(index);
    if (thisPost.numLikes != post.numLikes || thisPost.numReplies != post.numReplies || thisPost.isFollowedByCurrentUser != post.isFollowedByCurrentUser || thisPost.isLikedByCurrentUser != post.isLikedByCurrentUser || thisPost.isRebloggedByCurrentUser != post.isRebloggedByCurrentUser) {
      return false;
    }
  }
  return true;
}
