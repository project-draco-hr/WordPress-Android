{
  if (mNotificationsSettings == null || mNotificationsSettings.getOtherSettings() == null) {
    return;
  }
  PreferenceScreen otherBlogsScreen=(PreferenceScreen)findPreference(getString(R.string.pref_notification_other_blogs));
  addPreferencesForPreferenceScreen(otherBlogsScreen,Channel.OTHER,mNotificationsSettings.getOtherSettings(),0);
}
