{
  if (!isAdded() || mBlog == null || url == null) {
    return;
  }
  String authenticationUrl=WordPress.getLoginUrl(mBlog);
  String postData=WPWebViewActivity.getAuthenticationPostData(authenticationUrl,url,mBlog.getUsername(),mBlog.getPassword(),AccountHelper.getDefaultAccount().getAccessToken());
  mWebView.postUrl(authenticationUrl,postData.getBytes());
}
