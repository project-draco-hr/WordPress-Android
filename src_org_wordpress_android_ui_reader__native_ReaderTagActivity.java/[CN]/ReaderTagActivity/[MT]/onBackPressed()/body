{
  if (mTagsChanged) {
    Bundle bundle=new Bundle();
    bundle.putBoolean(KEY_TAGS_CHANGED,true);
    if (mLastAddedTag != null && ReaderTopicTable.topicExists(mLastAddedTag))     bundle.putString(KEY_LAST_ADDED_TAG,mLastAddedTag);
    Intent intent=new Intent();
    intent.putExtras(bundle);
    setResult(RESULT_OK,intent);
  }
  super.onBackPressed();
}
