{
  if (isVideo) {
    AnalyticsTracker.track(fromMediaLibrary ? Stat.EDITOR_ADDED_VIDEO_VIA_WP_MEDIA_LIBRARY : Stat.EDITOR_ADDED_VIDEO_VIA_LOCAL_LIBRARY);
  }
 else {
    AnalyticsTracker.track(fromMediaLibrary ? Stat.EDITOR_ADDED_PHOTO_VIA_WP_MEDIA_LIBRARY : Stat.EDITOR_ADDED_PHOTO_VIA_LOCAL_LIBRARY);
  }
}
