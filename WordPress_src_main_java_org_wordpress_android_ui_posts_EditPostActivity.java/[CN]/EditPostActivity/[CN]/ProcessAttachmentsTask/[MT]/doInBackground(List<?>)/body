{
  ArrayList<?> multi_stream=(ArrayList<?>)args[0].get(0);
  String type=(String)args[0].get(1);
  for (  Object streamUri : multi_stream) {
    if (streamUri instanceof Uri) {
      Uri imageUri=(Uri)streamUri;
      if (type != null) {
        Context context=mWeakContext.get();
        if (context != null) {
          addMedia(imageUri);
        }
      }
    }
  }
}
