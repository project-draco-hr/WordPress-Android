{
  if (mInstance == null) {
    if (mApplication == null) {
      AppLog.e(T.UTILS,"You must call HelpshiftHelper.init(Application application) before getInstance()");
    }
    mInstance=new HelpshiftHelper();
    Helpshift.install(mApplication,BuildConfig.HELPSHIFT_API_KEY,BuildConfig.HELPSHIFT_API_DOMAIN,BuildConfig.HELPSHIFT_API_ID);
  }
  return mInstance;
}
