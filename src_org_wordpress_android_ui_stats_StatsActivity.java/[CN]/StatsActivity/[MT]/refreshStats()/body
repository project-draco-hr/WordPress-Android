{
  if (WordPress.getCurrentBlog() == null)   return;
  String blogId=null;
  if (WordPress.getCurrentBlog().isDotcomFlag())   blogId=String.valueOf(WordPress.getCurrentBlog().getBlogId());
 else {
    blogId=getBlogIdFromJetpack();
    if (blogId == null) {
      verifyJetpackSettings();
    }
  }
  StatsRestHelper.getStatsSummary(blogId);
  if (mStatsViewFragment != null) {
    StatsViewType viewType=mStatsViewFragment.getViewType();
    StatsRestHelper.getStats(viewType,blogId);
  }
}
