{
  int views=0;
  int comments=0;
  int favs=0;
  int reblogs=0;
  if (result != null) {
    views=result.getViewsAllTime();
    comments=result.getCommentsAllTime();
  }
  mViewsTotalText.setText(views + "");
  mCommentsTotalText.setText(comments + "");
  mFavsTotalText.setText(favs + "");
  mReblogTotalText.setText(reblogs + "");
}
