{
  Uri uri;
switch (v.getId()) {
case R.id.about_url:
    uri=Uri.parse(URL_AUTOMATTIC);
  break;
case R.id.about_tos:
uri=Uri.parse(URL_TOS);
break;
case R.id.about_privacy:
uri=Uri.parse(URL_AUTOMATTIC + URL_PRIVACY_POLICY);
break;
default :
return;
}
startActivity(new Intent(Intent.ACTION_VIEW,uri));
}
