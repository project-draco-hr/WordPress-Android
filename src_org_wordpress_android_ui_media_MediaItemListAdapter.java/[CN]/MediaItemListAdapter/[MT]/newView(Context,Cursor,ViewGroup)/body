{
  LayoutInflater inflater=LayoutInflater.from(context);
  int viewType=getItemViewType(cursor);
  View view=null;
  if (viewType == Type.IMAGE.ordinal())   view=inflater.inflate(R.layout.media_listitem_image,root,false);
 else   view=inflater.inflate(R.layout.media_listitem_default,root,false);
  return view;
}
