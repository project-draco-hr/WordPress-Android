{
  Map<String,String> params=new HashMap<>();
  if (title != null && !title.equals(remoteSettings.title)) {
    params.put("blog_title",title);
  }
  if (tagline != null && !tagline.equals(remoteSettings.tagline)) {
    params.put("blog_tagline",tagline);
  }
  if (tagline != null && !tagline.equals(remoteSettings.tagline)) {
    params.put("blog_tagline",tagline);
  }
  return params;
}
