{
  return address.equals(other.address) && username.equals(other.username) && password.equals(other.password)&& title.equals(other.title)&& tagline.equals(other.tagline)&& languageId == other.languageId && privacy == other.privacy && location == other.location && defaultPostFormat.equals(other.defaultPostFormat) && defaultCategory == other.defaultCategory;
}
