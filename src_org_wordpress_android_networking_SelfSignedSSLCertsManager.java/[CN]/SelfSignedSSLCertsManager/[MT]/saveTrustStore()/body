{
  FileOutputStream out=null;
  try {
    out=new FileOutputStream(mLocalTrustStoreFile);
    mLocalKeyStore.store(out,Config.DB_SECRET.toCharArray());
  }
  finally {
    if (out != null) {
      try {
        out.close();
      }
 catch (      IOException e) {
        AppLog.e(T.UTILS,e);
      }
    }
  }
}
