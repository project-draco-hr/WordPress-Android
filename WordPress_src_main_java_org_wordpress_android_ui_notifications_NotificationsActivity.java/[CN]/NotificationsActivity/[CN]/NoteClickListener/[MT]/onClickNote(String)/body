{
  if (TextUtils.isEmpty(noteId))   return;
  if (SimperiumUtils.getNotesBucket() != null) {
    try {
      Note note=SimperiumUtils.getNotesBucket().get(noteId);
      openNote(note);
    }
 catch (    BucketObjectMissingException e) {
      AppLog.e(T.NOTIFS,"Note could not be found.");
    }
  }
}
