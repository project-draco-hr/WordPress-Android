{
  super.onCreate(savedInstanceState);
  AppLog.i(AppLog.T.NOTIFS,"Creating NotificationsActivity");
  setContentView(R.layout.notifications_activity);
  Toolbar toolbar=(Toolbar)findViewById(R.id.toolbar);
  setSupportActionBar(toolbar);
  getSupportActionBar().setDisplayShowTitleEnabled(true);
  getSupportActionBar().setDisplayHomeAsUpEnabled(true);
  getSupportActionBar().setTitle(getResources().getString(R.string.notifications));
  if (savedInstanceState == null) {
    mNotesListFragment=new NotificationsListFragment();
    getFragmentManager().beginTransaction().add(R.id.notifications_container,mNotesListFragment,TAG_NOTES_LIST).commit();
    launchWithNoteId();
    AnalyticsTracker.track(AnalyticsTracker.Stat.NOTIFICATIONS_ACCESSED);
  }
 else {
    mNotesListFragment=(NotificationsListFragment)getFragmentManager().findFragmentByTag(TAG_NOTES_LIST);
  }
}
