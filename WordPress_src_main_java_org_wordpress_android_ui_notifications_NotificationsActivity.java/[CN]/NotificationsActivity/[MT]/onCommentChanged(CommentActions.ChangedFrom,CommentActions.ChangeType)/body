{
  if ((changeType == CommentActions.ChangeType.TRASHED || changeType == CommentActions.ChangeType.SPAMMED) && changedFrom == CommentActions.ChangedFrom.COMMENT_DETAIL) {
    FragmentManager fm=getFragmentManager();
    if (fm.getBackStackEntryCount() > 0) {
      fm.popBackStack();
    }
  }
  mNotesListFragment.refreshNotes();
}
