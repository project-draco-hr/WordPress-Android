{
  Fragment f=null;
switch (location) {
case 0:
    f=WPCOMReaderTopicsSelector.newInstance();
  topicPage=f;
break;
case 1:
f=WPCOMReaderImpl.newInstance();
readerPage=f;
break;
case 2:
f=WPCOMReaderDetailPage.newInstance();
detailPage=f;
break;
case 3:
f=WPCOMReaderWebPage.newInstance();
webPage=f;
break;
}
return f;
}
