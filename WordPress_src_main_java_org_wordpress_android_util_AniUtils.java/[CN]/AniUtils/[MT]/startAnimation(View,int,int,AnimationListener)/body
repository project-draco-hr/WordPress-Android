{
  if (target == null) {
    return;
  }
  Animation animation=AnimationUtils.loadAnimation(target.getContext(),aniResId);
  if (animation == null) {
    return;
  }
  if (listener != null) {
    animation.setAnimationListener(listener);
  }
  animation.setDuration(duration);
  target.startAnimation(animation);
}
