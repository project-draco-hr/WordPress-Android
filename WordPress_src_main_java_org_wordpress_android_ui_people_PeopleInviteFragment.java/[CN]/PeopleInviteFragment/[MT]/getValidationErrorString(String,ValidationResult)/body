{
switch (validationResult) {
case USER_NOT_FOUND:
    return getString(R.string.invite_username_not_found,username);
case ALREADY_MEMBER:
  return getString(R.string.invite_already_a_member,username);
case INVALID_EMAIL:
return getString(R.string.invite_invalid_email,username);
case USER_FOUND:
return FLAG_SUCCESS;
}
return null;
}
