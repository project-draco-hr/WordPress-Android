{
  if (!isAdded()) {
    return;
  }
  if (mUsernameEditText.getText().toString().length() > 0) {
    addUsername(mUsernameEditText,LayoutInflater.from(getActivity()),mUsernamesContainer,new ValidationEndListener(){
      @Override public void onValidationEnd(){
        checkAndSend();
      }
    }
);
  }
 else {
    checkAndSend();
  }
}
