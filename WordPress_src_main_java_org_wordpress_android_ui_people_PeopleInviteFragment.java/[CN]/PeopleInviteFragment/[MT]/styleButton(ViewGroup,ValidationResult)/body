{
  if (!isAdded()) {
    return;
  }
  TextView textView=(TextView)button.findViewById(R.id.username);
switch (validationResult) {
case USER_NOT_FOUND:
case ALREADY_MEMBER:
case INVALID_EMAIL:
    textView.setTextColor(ContextCompat.getColor(getActivity(),R.color.alert_red));
  break;
case USER_FOUND:
break;
}
}
