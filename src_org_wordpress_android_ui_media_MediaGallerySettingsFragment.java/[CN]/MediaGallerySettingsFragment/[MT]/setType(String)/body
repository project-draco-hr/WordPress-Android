{
  mType=GalleryType.getTypeFromTag(type);
switch (mType) {
case CIRCLES:
    mCirclesCheckbox.setChecked(true);
  break;
case DEFAULT:
mThumbnailCheckbox.setChecked(true);
break;
case SLIDESHOW:
mSlideshowCheckbox.setChecked(true);
break;
case SQUARES:
mSquaresCheckbox.setChecked(true);
break;
case TILED:
mTiledCheckbox.setChecked(true);
break;
}
}
